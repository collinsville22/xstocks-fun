# Frontend Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to git - it's in .gitignore

# =============================================================================
# CRITICAL: API Keys and Secrets
# =============================================================================

# Helius RPC URL with your API key
# Get your key from: https://helius.dev
VITE_HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY_HERE

# Logo.dev API Token (optional, for stock logos)
# Get your token from: https://logo.dev
VITE_LOGO_DEV_TOKEN=YOUR_LOGO_DEV_TOKEN_HERE

# =============================================================================
# Backend Service URLs
# =============================================================================

# Intel Microservice API URL
# Production: Leave empty to use relative URL (/api/intel/...)
# Development: Set to http://localhost:8000
VITE_INTEL_API_URL=

# Backend Trading Service URL
# Production: https://your-backend.vercel.app
# Development: http://localhost:3008
VITE_BACKEND_URL=http://localhost:3008

# WebSocket URL for real-time updates
# Production: wss://your-backend.vercel.app
# Development: ws://localhost:3008
VITE_WS_URL=ws://localhost:3008

# =============================================================================
# External Services (Already configured with public endpoints)
# =============================================================================

# Jupiter APIs (public endpoints)
NEXT_PUBLIC_JUPITER_ULTRA_API=https://lite-api.jup.ag/ultra/v1
NEXT_PUBLIC_JUPITER_PRICE_API=https://api.jup.ag/price/v2

# Wormhole APIs
VITE_WORMHOLE_SCAN_API=https://api.wormholescan.io
VITE_GUARDIAN_API=https://wormhole-v2-mainnet-api.certus.one

# =============================================================================
# Development Settings
# =============================================================================

# Node environment
NODE_ENV=development
